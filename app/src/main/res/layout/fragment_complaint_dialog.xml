<androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     app:cardBackgroundColor="#FFFFFF
"
    android:animateLayoutChanges="true"
    app:cardElevation="@dimen/_8sdp"
    tools:context=".admin.AddUserFragment">

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
         android:paddingStart="@dimen/_15sdp"
        android:paddingLeft="@dimen/_15sdp"
         tools:context=".admin.AddUserFragment">


        <ImageButton
            android:id="@+id/ib_dismiss"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_alignParentEnd="true"
            android:layout_margin="@dimen/_2sdp"
            android:backgroundTint="@android:color/transparent"
            android:src="@drawable/ic_clear_black_24dp"/>


        <TextView
            android:id="@+id/tv_complaint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:textSize="@dimen/_17ssp"
             android:maxLines="5"
            android:textColor="#000"
            android:fontFamily="@font/coresansar"
            android:layout_marginTop="@dimen/_18sdp"
              android:layout_below="@id/ib_dismiss"
             android:text="Internet not working"
            />

        <TextView
            android:id="@+id/lbl_complaint_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_15ssp"
            android:fontFamily="@font/coresansar"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/_28sdp"
            android:layout_below="@id/tv_complaint"
            android:text="Complaint Id"/>

        <TextView
            android:id="@+id/tv_complaint_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:textSize="@dimen/_13ssp"
             android:layout_marginEnd="@dimen/_18sdp"
            android:fontFamily="@font/roboto_regular"

            android:layout_alignTop="@id/lbl_complaint_id"
            android:text=""/>

        <TextView
            android:id="@+id/lbl_customer_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_15ssp"
            android:fontFamily="@font/coresansar"
            android:textStyle="bold"

            android:layout_marginTop="@dimen/_12sdp"
            android:layout_below="@id/lbl_complaint_id"
            android:text="Customer Id"/>

        <TextView
            android:id="@+id/tv_customer_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_13ssp"
            android:layout_marginEnd="@dimen/_18sdp"
            android:fontFamily="@font/roboto_regular"

            android:layout_alignParentEnd="true"
            android:layout_alignTop="@id/lbl_customer_id"
            android:text=""/>

        <TextView
            android:id="@+id/lbl_customer_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_15ssp"
            android:fontFamily="@font/coresansar"
            android:textStyle="bold"

            android:layout_marginTop="@dimen/_12sdp"
            android:layout_below="@id/lbl_customer_id"
            android:text="Phone"/>

        <TextView
            android:id="@+id/tv_customer_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_13ssp"
            android:fontFamily="@font/roboto_regular"
            android:layout_marginEnd="@dimen/_18sdp"

            android:layout_alignParentEnd="true"
            android:layout_alignTop="@id/lbl_customer_phone"
            android:text=""/>


        <TextView
            android:id="@+id/lbl_complaint_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_15ssp"
            android:fontFamily="@font/coresansar"
            android:layout_marginTop="@dimen/_12sdp"
            android:textStyle="bold"
            android:layout_below="@id/lbl_customer_phone"
            android:text="Date"/>

        <TextView
            android:id="@+id/tv_complaint_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_13ssp"
            android:fontFamily="@font/roboto_regular"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="@dimen/_18sdp"
            android:layout_alignTop="@id/lbl_complaint_time"
            android:text=""/>





    </RelativeLayout>
</androidx.cardview.widget.CardView>